<script type="text/javascript">
    //animation
    $(function () { //auto hide
        $("#precast_add,#cast1_add,#cast2_add").hide();
        //show
        $("#tab1").live
          ("click", function () {
              $("#fuelred_div_aflogsheet,#lpg_div_aflogsheet,#afadd").show();
              $("#precast_add,#cast1_add,#cast2_add").hide();
              $("#afcustomer").css('visibility', 'hidden');
          });
        //click to hide
        $("#tab2").live
          ("click", function () {
              $("#fuelred_div_aflogsheet,#lpg_div_aflogsheet,#afadd,#precast_add").hide();
              $("#afcustomer").css('visibility', 'visible');
              $("#cast1_add,#cast2_add").show();
              var aflot = $('#cfcyclenum_cycledetail').text();


          });

        $("#tab3").live
          ("click", function () {
              $("#fuelred_div_aflogsheet,#lpg_div_aflogsheet,#afadd,#cast1_add,#cast2_add").hide();
              $("#precast_add").css('visibility', 'visible');
              $("#precast_add").show();
              $("#afcustomer").css('visibility', 'visible');        
              var aflot = $('#cfcyclenum_cycledetail').text();

              load_ladle_measure(aflot);
              load_mouldr(aflot);
              precast_customer_data(aflot);
              load_PreCast_Act_for_tab(aflot);
              
              
          });



    });
    //end

    //history Back
    function goBack() {
        window.history.back();
